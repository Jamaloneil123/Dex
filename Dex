-- sliders -- 

Playerstuff:AddSlider({
	Name = "Walkspeed",
	Min = 16,
	Max = 1000,
	Default = 16,
	Color = Color3.fromRGB(255, 255, 255),
	Increment = 2,
	ValueName = "Walk speed",
	Callback = function(Value)
		loopSpeedValue = Value -- Update the global speed value
		if loopSpeedEnabled then
			-- If the loop is enabled, apply the updated speed immediately
			local speaker = game.Players.LocalPlayer
			local Char = speaker.Character or workspace:FindFirstChild(speaker.Name)
			local Human = Char and Char:FindFirstChildWhichIsA("Humanoid")
			if Human then
				Human.WalkSpeed = loopSpeedValue
			end
		end
	end    
})

Playerstuff:AddSlider({
	Name = "JumpPower",
	Min = 50,
	Max = 500,
	Default = 50,
	Color = Color3.fromRGB(255, 255, 255),
	Increment = 2,
	ValueName = "Jump Power",
	Callback = function(Value)
		loopJumpPowerValue = Value -- Update the global JumpPower value
		if loopJumpPowerEnabled then
			-- If the loop is enabled, apply the updated JumpPower immediately
			local speaker = game.Players.LocalPlayer
			local Char = speaker.Character or workspace:FindFirstChild(speaker.Name)
			local Human = Char and Char:FindFirstChildWhichIsA("Humanoid")
			if Human then
				Human.JumpPower = loopJumpPowerValue
			end
		end
	end    
})

-- paragraph --

changelog:AddParagraph("Change logs/Update log","Added Void killer with toggle and bind also added walk speed and jump power with both toggle and value slider")


-- toggles -- 


Playerstuff:AddToggle({
	Name = "Loop Walkspeed",
	Default = false,
	Callback = function(Value)
		loopSpeedEnabled = Value -- Update the toggle state

		-- Get the player and humanoid
		local speaker = game.Players.LocalPlayer
		local Char = speaker.Character or workspace:FindFirstChild(speaker.Name)
		local Human = Char and Char:FindFirstChildWhichIsA("Humanoid")

		-- Function to enforce WalkSpeed
		local function ApplyWalkSpeed()
			if Char and Human then
				Human.WalkSpeed = loopSpeedValue
			end
		end

		if loopSpeedEnabled then
			-- Enable the loop
			if Human then
				ApplyWalkSpeed() -- Apply once immediately
			end
			-- Connect the loop to enforce WalkSpeed changes
			loopConnection = Human:GetPropertyChangedSignal("WalkSpeed"):Connect(function()
				ApplyWalkSpeed()
			end)

			-- Handle respawning
			speaker.CharacterAdded:Connect(function(newChar)
				Char, Human = newChar, newChar:WaitForChild("Humanoid")
				ApplyWalkSpeed()
				loopConnection = Human:GetPropertyChangedSignal("WalkSpeed"):Connect(function()
					ApplyWalkSpeed()
				end)
			end)
		else
			-- Disable the loop
			if loopConnection then
				loopConnection:Disconnect()
				loopConnection = nil
			end
		end
	end
})

Playerstuff:AddToggle({
	Name = "Loop JumpPower",
	Default = false,
	Callback = function(Value)
		loopJumpPowerEnabled = Value -- Update the toggle state

		-- Get the player and humanoid
		local speaker = game.Players.LocalPlayer
		local Char = speaker.Character or workspace:FindFirstChild(speaker.Name)
		local Human = Char and Char:FindFirstChildWhichIsA("Humanoid")

		-- Function to enforce JumpPower
		local function ApplyJumpPower()
			if Char and Human then
				Human.JumpPower = loopJumpPowerValue
			end
		end

		if loopJumpPowerEnabled then
			-- Enable the loop
			if Human then
				ApplyJumpPower() -- Apply once immediately
			end
			-- Connect the loop to enforce JumpPower changes
			loopJumpConnection = Human:GetPropertyChangedSignal("JumpPower"):Connect(function()
				ApplyJumpPower()
			end)

			-- Handle respawning
			speaker.CharacterAdded:Connect(function(newChar)
				Char, Human = newChar, newChar:WaitForChild("Humanoid")
				ApplyJumpPower()
				loopJumpConnection = Human:GetPropertyChangedSignal("JumpPower"):Connect(function()
					ApplyJumpPower()
				end)
			end)
		else
			-- Disable the loop
			if loopJumpConnection then
				loopJumpConnection:Disconnect()
				loopJumpConnection = nil
			end
		end
	end
})

-- keybinds --

local function teleportToVoid()
    local character = player.Character or player.CharacterAdded:Wait()
    local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
    
    if humanoidRootPart then
        -- Teleport to the void (Y = -500)
        humanoidRootPart.CFrame = CFrame.new(humanoidRootPart.Position.X, -500, humanoidRootPart.Position.Z)
    end
end

Keybinds:AddToggle({
	Name = "Void Kill Toggle",
	Default = false,
	Callback = function(Value)
		VoidKillEnabled = Value -- Update the toggle state
	end
})

Keybinds:AddBind({
	Name = "Void Kill",
	Default = Enum.KeyCode.E, -- Default keybind is "E"
	Hold = false,
	Callback = function()
		if VoidKillEnabled then
			teleportToVoid() -- Only teleport if the toggle is enabled
		end
	end
})


