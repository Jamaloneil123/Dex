-- Function to find ReplicatedStorage or renamed versions
local function findReplicatedStorage()
    -- Search the game hierarchy for instances with "ReplicatedStorage" in their name
    for _, child in pairs(game:GetChildren()) do
        if string.lower(child.Name):find("replicatedstorage") then
            return child -- Return the found instance
        end
    end
    return nil -- Return nil if no match is found
end

-- Attempt to locate ReplicatedStorage
local ReplicatedStorage = findReplicatedStorage()

-- Check if ReplicatedStorage was found
if ReplicatedStorage then
    print("ReplicatedStorage found: " .. ReplicatedStorage.Name)
else
    warn("ReplicatedStorage could not be found.")
end

-- Function to find Players or renamed versions
local function findPlayers()
    -- Search the game hierarchy for instances with "Players" in their name
    for _, child in pairs(game:GetChildren()) do
        if string.lower(child.Name):find("players") then
            return child -- Return the found instance
        end
    end
    return nil -- Return nil if no match is found
end

-- Attempt to locate Players
local PlayersService = findPlayers()

-- Check if Players was found
if PlayersService then
    print("Players service found: " .. PlayersService.Name)
else
    warn("Players service could not be found.")
end






local Players = game:GetService("Players")
local player = Players.LocalPlayer
local Players = PlayersService

if game.PlaceId == 10449761463 then
    GameName = "TBS"
elseif game.PlaceId == 17625359962 then
    GameName = "RIVALS"
else
    GameName = "unknown"
end


-- List of UserIds who should have access to the premium features
local premiumUserIds = {
    12345678, -- Replace with actual UserIds of Premium players
    7690555926, -- Add more UserIds here as needed
}

-- List of Owner UserIds
local ownerUserIds = {
    7690555926, -- Replace with the first owner's UserId
    7629016664, -- Replace with the second owner's UserId
}

-- Function to check if the player is a premium user
local function isPremiumUser(player)
    for _, id in ipairs(premiumUserIds) do
        if player.UserId == id then
            return true  -- The player has access to the premium features
        end
    end
    return false  -- The player does not have access to the premium features
end

-- Function to check if the player is an owner
local function isOwner(player)
    for _, id in ipairs(ownerUserIds) do
        if player.UserId == id then
            return true  -- The player is an owner
        end
    end
    return false  -- The player is not an owner
end

-- Function to toggle PremiumOnly and OwnerOnly properties based on UserId check
local function togglePremiumOnly(player)
    local isPremium = isPremiumUser(player)  -- Check if the player is a premium user
    local isOwner = isOwner(player)          -- Check if the player is an owner

    if isPremium then
        PremiumOnly1 = false  -- Allow access for premium users
    else
        PremiumOnly1 = true   -- Restrict access for non-premium users
    end

    if isOwner then
        OwnerOnly1 = false  -- Allow access for owners
    else
        OwnerOnly1 = true   -- Restrict access for non-owners
    end
end

-- Example usage
local player = game.Players.LocalPlayer -- Replace with your player reference
togglePremiumOnly(player)

